{
  "name": "cardcutter-extension",
  "description": "NSDA Debate Card Cutter - Browser extension for formatting evidence citations and highlighting",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@tsconfig/svelte": "^5.0.5",
    "@wxt-dev/module-svelte": "^2.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.25",
    "@ai-sdk/google": "^2.0.18",
    "@ai-sdk/openai": "^2.0.46",
    "ai": "^5.0.65",
    "lucide-svelte": "^0.545.0"
  }
}
